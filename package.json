  "bugs": {
    "url": "https://github.com/littlefish-tech/todolist/issues"
  },
  "homepage": "https://github.com/littlefish-tech/todolist#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.3.1",
    "express": "^4.17.1",
    "express-validator": "^6.5.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^5.9.16",
    "react-calendar": "^3.1.0"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4"
  }
